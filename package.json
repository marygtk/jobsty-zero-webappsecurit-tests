{
  "name": "jobsty-zero-webappsecurit-tests",
  "version": "1.0.0",
  "description": "QA Challenge for jobsity ",
  "main": "index.js",
  "scripts": {
    "test": "cy:run",
    "clean-results": "rm -rf allure-results/*",
    "clean-report": "rm -rf allure-report/*",
    "create-report": "allure generate --clean && allure open",
    "cy:allure:run": "npx cypress run --config video=false --env allure=true",
    "cy:run:automationpractice": "cypress run --browser chrome --headless --spec 'cypress/integration/examples/automationPractice/*'",
    "cy:run:zerobank": "cypress run --browser chrome --headless --spec 'cypress/integration/examples/zeroBank/*'"

